    "@types/node": "^10.0.2",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "prettier": "^1.12.1",
    "react-scripts-ts": "^2.15.1",
    "ts-config-mobiquity-react-native": "^0.0.18",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
